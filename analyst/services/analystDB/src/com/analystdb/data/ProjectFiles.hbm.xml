<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.ProjectFiles" table="project_files" dynamic-insert="false" dynamic-update="false">
        <id name="name" type="integer">
            <column name="name"/>
            <generator class="assigned"/>
        </id>
        <property name="description" type="string">
            <column name="description"/>
        </property>
        <property name="content" type="binary">
            <column name="content"/>
        </property>
        <many-to-one name="project" class="com.analystdb.data.Project">
            <column name="project" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.IssueApproach" table="issue_approach" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.analystdb.data.IssueApproachId">
            <key-property name="issueId" type="long">
                <column name="issue_id"/>
            </key-property>
            <key-property name="approachId" type="long">
                <column name="approach_id"/>
            </key-property>
        </composite-id>
        <many-to-one
            name="issue"
            class="com.analystdb.data.Issue"
            update="false"
            insert="false"
            fetch="select">
            <column name="issue_id" not-null="true"/>
        </many-to-one>
        <many-to-one
            name="approach"
            class="com.analystdb.data.Approach"
            update="false"
            insert="false"
            fetch="select">
            <column name="approach_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
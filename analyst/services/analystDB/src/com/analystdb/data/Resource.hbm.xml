<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.Resource" table="resource" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="name" length="127" not-null="true"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
        <property name="version" type="integer">
            <column name="version"/>
        </property>
        <many-to-one name="project" class="com.analystdb.data.Project">
            <column name="project" not-null="true"/>
        </many-to-one>
        <set name="interviews" inverse="true" cascade="">
            <key>
                <column name="resource" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.Interview"/>
        </set>
        <set name="resourceAttributes" inverse="true" cascade="">
            <key>
                <column name="resource" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.ResourceAttribute"/>
        </set>
    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.Approach" table="approach" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="name" length="127" not-null="true"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
        <property name="version" type="integer">
            <column name="version"/>
        </property>
        <many-to-one name="interview" class="com.analystdb.data.Interview">
            <column name="interview" not-null="true"/>
        </many-to-one>
        <set name="issueApproachs" inverse="true" cascade="">
            <key>
                <column name="approach_id" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.IssueApproach"/>
        </set>
    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.ResourceAttribute" table="resource_attribute" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="name" length="127" not-null="true"/>
        </property>
        <property name="val" type="string">
            <column name="val" not-null="true"/>
        </property>
        <property name="version" type="integer">
            <column name="version"/>
        </property>
        <many-to-one name="resource" class="com.analystdb.data.Resource">
            <column name="resource" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.Project" table="project" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="name" length="30" not-null="true"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
        <property name="version" type="integer">
            <column name="version"/>
        </property>
        <property name="tenantId" type="integer">
            <column name="tenant_id" not-null="true"/>
        </property>
        <set name="issueCategories" inverse="true" cascade="">
            <key>
                <column name="project" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.IssueCategory"/>
        </set>
        <set name="analysises" inverse="true" cascade="">
            <key>
                <column name="project" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.Analysis"/>
        </set>
        <set name="issues" inverse="true" cascade="">
            <key>
                <column name="project" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.Issue"/>
        </set>
        <set name="resources" inverse="true" cascade="">
            <key>
                <column name="project" not-null="true"/>
            </key>
            <one-to-many class="com.analystdb.data.Resource"/>
        </set>
    </class>
</hibernate-mapping>
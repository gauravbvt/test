<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.Documents" table="documents" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="document" type="string">
            <column name="document" length="63"/>
        </property>
        <many-to-one name="documentCategory" class="com.analystdb.data.DocumentCategory">
            <column name="category"/>
        </many-to-one>
        <set name="flows" inverse="true" cascade="">
            <key>
                <column name="document"/>
            </key>
            <one-to-many class="com.analystdb.data.Flow"/>
        </set>
    </class>
</hibernate-mapping>
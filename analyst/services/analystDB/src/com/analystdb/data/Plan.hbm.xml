<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.Plan" table="plan" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
        <property name="name" type="string">
            <column name="name" length="127" not-null="true"/>
        </property>
        <property name="approved" type="boolean">
            <column name="approved" not-null="true"/>
        </property>
        <property name="version" type="integer">
            <column name="version"/>
        </property>
        <many-to-one name="analysis" class="com.analystdb.data.Analysis">
            <column name="analysis" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.analystdb.data.Tenant" table="tenant" dynamic-insert="false" dynamic-update="false">
        <id name="tenantId" type="integer">
            <column name="tenant_id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="name" length="127" not-null="true"/>
        </property>
        <property name="logo" type="string">
            <column name="logo"/>
        </property>
        <property name="disabled" type="boolean">
            <column name="disabled"/>
        </property>
        <property name="expires" type="timestamp">
            <column name="expires" length="19"/>
        </property>
    </class>
</hibernate-mapping>
<?xml version="1.0"?><project>
  <parent>
    <artifactId>generic</artifactId>
    <groupId>com.mindalliance</groupId>
    <version>1.0-SNAPSHOT</version>
    <relativePath>../generic/pom.xml</relativePath>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.mindalliance.channels</groupId>
  <artifactId>channels</artifactId>
  <packaging>war</packaging>
  <name>Channels</name>
  <version>1.0-SNAPSHOT</version>
  <description>Communications Analysis and Planning</description>
  <url>https://ranger.homelinux.net/projects/channels</url>
  <scm>
    <connection>scm:svn:http://ranger.homelinux.net/svn/${project.artifactId}/trunk</connection>
    <developerConnection>scm:svn:https://ranger.homelinux.net/svn/${project.artifactId}/trunk</developerConnection>
    <url>https://ranger.homelinux.net/tracsvn/${project.artifactId}</url>
  </scm>
  <issueManagement>
	<system>TRAC</system>
	<url>https://ranger.homelinux.net/trac/${project.artifactId}/report</url>
  </issueManagement>
  <build>
    <resources>
      <resource>
        <directory>src/main/java</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <configuration>
          <outputDirectory>target/site</outputDirectory>
          <webResources>
            <resource>
              <directory>src/main/webapp</directory>
              <filtering>true</filtering>
              <includes>
                <include>**/*.xml</include>
                <include>**/*.jsp</include>
              </includes>
            </resource>
          </webResources>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>aspectj-maven-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>compile</goal>
              <goal>test-compile</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <source>1.5</source>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.mortbay.jetty</groupId>
        <artifactId>maven-jetty-plugin</artifactId>
        <version>6.1.2</version>
        <configuration>
          <scanIntervalSeconds>10</scanIntervalSeconds>
          <connectors>
            <connector implementation="org.mortbay.jetty.nio.SelectChannelConnector">
              <port>8080</port>
              <maxIdleTime>60000</maxIdleTime>
            </connector>
          </connectors>
        </configuration>
      </plugin>
    </plugins>
  </build>

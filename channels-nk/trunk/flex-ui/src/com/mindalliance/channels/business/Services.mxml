<?xml version="1.0" encoding="utf-8"?>
<cairngorm:ServiceLocator xmlns:mx="http://www.adobe.com/2006/mxml" 
	            xmlns:cairngorm="com.adobe.cairngorm.business.*" >
	<mx:Script>
		<![CDATA[
			import com.mindalliance.channels.model.ChannelsModelLocator;
			
			public static const GET_PROJECT_LIST_SERVICE : String = "getProjectListService";
			public static const GET_PROJECT_SERVICE : String = "getProjectService";
			public static const GET_SCENARIO_LIST_SERVICE : String = "getScenarioListService";
			public static const GET_SCENARIO_SERVICE : String = "getScenarioService";
			
 	        [Bindable]
 	        private var model : ChannelsModelLocator = ChannelsModelLocator.getInstance();
 	      
		]]>
	</mx:Script>
	
	<mx:HTTPService id="getProjectListService" url="assets/xml/projectList.xml"/>
	<mx:HTTPService id="getProjectService" url="assets/xml/project{model.projectScenarioBrowserModel.selectedProjectId}.xml"/>
	<mx:HTTPService id="getScenarioListService" url="assets/xml/scenarioList{model.projectScenarioBrowserModel.selectedProjectId}.xml"/>
	<mx:HTTPService id="getScenarioService" url="assets/xml/scenario{model.projectScenarioBrowserModel.selectedScenarioId}.xml"/>

</cairngorm:ServiceLocator>
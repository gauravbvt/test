<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:view="com.mindalliance.channels.view.*"
	xmlns:flowmap="com.mindalliance.channels.view.flowmap.*"
	width="100%" height="100%"
	creationComplete="init()">
	
	<mx:Script>
		<![CDATA[
			private function init():void {
				zoomControl.graphCanvas = this.graphCanvas ;
			}
		]]>
	</mx:Script>
	
	<flowmap:ZoomControl width="100%" borderSides="bottom" borderThickness="1" borderStyle="solid" id="zoomControl"/>
	<flowmap:FlowMap id="graphCanvas" width="100%" height="100%"/>
	<!-- <mx:Image id="graphCanvas" source="assets/exampleflowmap2.png" width="100%" height="100%" scaleContent="false"/> -->
</mx:VBox>

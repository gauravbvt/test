<?xml version="1.0" encoding="UTF-8"?><module>
	<!--
	***********
	Mind-Alliance Geonames Module
	***********
	-->
    <identity>
		<uri>urn:com:mindalliance:channels:geonames</uri>
		<version>0.0.13</version>
    </identity>
	<info>
		<name>Mind-Alliance Geonames Module</name>
        <description>A module containing the basic structure for a NetKernel module</description>
		<dynamic/>
    </info>
    <publisher>
        <name>Mind-Alliance, Inc.</name>
		<uri>http://www.mindalliance.com</uri>
    </publisher>
	<export>
		<uri>
			<match>ffcpl:/geonames/search.*</match>
			<match>active:geonamesSearch\+.*</match>
			<match>ffcpl:/etc/HTTPBridgeConfig.xml</match>
			<match>ffcpl:/entrypoints.xml</match>
		</uri>
	</export>
	<mapping>
		<this>
			<match>ffcpl:/entrypoints.xml</match>
			<match>ffcpl:/etc/.*</match>
		</this>
		<this>
			<match>ffcpl:/resources/.*</match>
		</this>
		<rewrite>
			<match>ffcpl:/geonames/search(.*)</match>
			<to>active:geonamesSearch$1</to>
		</rewrite>
		<ura>
      		<match>active:geonamesSearch\+.*</match>
      		<class>com.mindalliance.channels.geonames.accessor.Search</class>
    	</ura>
        <import>
            <uri>urn:com:mindalliance:channels:support</uri>
        </import>
        <import>
			<uri>urn:org:ten60:netkernel:ext:session</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:script</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:sys</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:core</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:ura</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:layer1</uri>
		</import>
		<super/>
    </mapping>
</module>
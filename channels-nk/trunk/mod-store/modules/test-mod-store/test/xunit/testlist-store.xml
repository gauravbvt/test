<?xml version="1.0" encoding="UTF-8"?>
<testlist  title="Store Accessor Unit Tests">
    <desc>
	<div>
	<b>Store</b> accessor unit tests for the <b>Mind-Alliance Store</b> lib.
	</div>
    </desc>
    <test>
        <uri>active:beanshell+operator@ffcpl:/test/accessor/test.bsh</uri>
		<assert>
			<xpath>count(//hello) = 1</xpath>
		</assert>
    </test>
    <test>
      <uri>active:javascript+operator@ffcpl:/test/accessor/store-test1.js</uri>
		<assert>
			<xpath>count(//hello) = 1</xpath>
		</assert>
    </test>
    <test>
      <uri>active:javascript+operator@ffcpl:/test/accessor/store-test1.js</uri>
		<assert>
			<xpath>//hello = 'store'</xpath>
		</assert>
    </test>
</testlist>
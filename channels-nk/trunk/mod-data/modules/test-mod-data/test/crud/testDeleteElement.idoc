<idoc>
  <comment>Create an element and delete it using its id</comment>
  <seq>
    <instr>
      <type>channels_data_createElement</type>
      <doc>
        <project>
          <name>My project</name>
        </project>
      </doc>
      <kind><string>project</string></kind>
      <target>var:element</target>
    </instr>
    <instr>
      <type>copy</type>
      <operand>var:element#xpointer(./id[1])</operand>
      <target>var:id</target>
    </instr>
    <instr>
      <type>channels_data_deleteElement</type>
      <id>var:id</id>
     </instr>
    <seq> 
	    <instr>
	      <type>channels_data_getElement</type>
	      <id>var:id</id>
	      <target>this:response</target>
	     </instr>
	     <exception>
			    <instr>
			      <type>throw</type>
			      <operand>
			        <ex>
			          <id>Element not found</id>
			          <message>Can't get element</message>
			        </ex>
			      </operand>
			    </instr>
	     </exception>
	   </seq>
  </seq>
</idoc>

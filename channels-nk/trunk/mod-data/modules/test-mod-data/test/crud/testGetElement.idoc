<idoc>
  <comment>Create an element and get it back using its id</comment>
  <seq>
    <instr>
      <type>channels_data_createElement</type>
      <doc>
        <project>
          <name>My project</name>
        </project>
      </doc>
      <kind><string>project</string></kind>
      <target>var:project</target>
    </instr>
    <instr>
      <type>copy</type>
      <operand>var:project#xpointer(./id[1])</operand>
      <target>var:id</target>
    </instr>
    <instr>
        <type>beanshell</type>
        <operator>testGetElement.bsh</operator>
        <id>var:id</id>
        <target>this:response</target>
    </instr>
  </seq>
</idoc>

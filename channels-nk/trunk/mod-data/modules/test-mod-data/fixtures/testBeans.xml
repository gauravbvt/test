<beans>
    <!-- TestEnvironment -->
    <testEnvironment version='1.0.0' beanClass='test.TestEnvironment' id='TestEnvironment' rooted='true'>
        <tests itemClass='BeanReference' itemName='test'>
            <test beanRef="test.TestBean"><id>Top</id></test>
            <test beanRef="test.TestBean"><id>SubA</id></test>
            <test beanRef="test.TestBean"><id>SubB</id></test>
            <test beanRef="test.TestBean"><id>SubSub</id></test>
        </tests>
    </testEnvironment>
    <!--
      Top <- SubA , SubB
    -->
    <!-- Top - rooted at 'Top' -->
    <test.TestBean version='1.0.0' beanClass='test.TestBean' id='Top' rooted='true'>
        <name dataType='String'>Top</name>
        <kind dataType='String'>Integration</kind>
        <successful dataType='Boolean'>true</successful>
        <cost dataType='Double'>60.0</cost>
        <runs itemClass='test.TestRunComponent' itemName='run'>
            <run beanClass='test.TestRunComponent'>
                <date dataType='Date'>Mon Jan 21 15:00:00 EST 2008</date>
                <tester dataType='String'>Joe Integrator</tester>
            </run>
        </runs>
        <subTests itemClass='BeanReference' itemName='test' itemDomain='_,_,_,subTests_domain.groovy'>
            <test beanRef='test.TestBean'><id>SubA</id></test>
            <test beanRef='test.TestBean'><id>SubB</id></test>
        </subTests>
    </test.TestBean>
    <!-- SubA -->
    <test.TestBean version='1.0.0' beanClass='test.TestBean' id='SubA'>
        <name dataType='String'>SubA</name>
        <kind dataType='String'>Unit</kind>
        <parent beanRef='test.TestBean' domain='_,_,_,parents_domain.groovy'><id>Top</id></parent>
        <successful dataType='Boolean'>true</successful>
        <cost dataType='Double'>30.0</cost>
        <runs itemClass='test.TestRunComponent' itemName='run'>
            <run beanClass='test.TestRunComponent'>
                <date dataType='Date'>Mon Jan 21 14:00:00 EST 2008</date>
                <tester dataType='String'>John U. Test</tester>
            </run>
            <run beanClass='test.TestRunComponent'>
                <date dataType='Date'>Mon Jan 21 14:30:00 EST 2008</date>
                <tester dataType='String'>Jane Q. Hay</tester>
            </run>
        </runs>
        <subTests itemClass='BeanReference' itemName="test" itemDomain='_,_,_,subTests_domain.groovy'/>
    </test.TestBean>

    <!-- SubB <- SubSub -->
    <test.TestBean version='1.0.0' beanClass='test.TestBean' id="SubB">
        <name dataType='String'>SubB</name>
        <kind dataType='String'>Integration</kind>
        <parent beanRef='test.TestBean' domain='_,_,_,parents_domain.groovy'><id>Top</id></parent>
        <successful dataType='Boolean'>true</successful>
        <cost dataType='Double'>100.0</cost>
        <runs itemClass='test.TestRunComponent' itemName='run'>
            <run beanClass='test.TestRunComponent'>
                <date dataType='Date'>Mon Jan 21 13:00:00 EST 2008</date>
                <tester dataType='String'>John U. Test</tester>
            </run>
        </runs>
         <subTests itemClass='BeanReference' itemName="test" itemDomain='_,_,_,subTests_domain.groovy'>
            <test beanRef='test.TestBean'><id>SubSub</id></test>
        </subTests>
    </test.TestBean>

    <!-- SubSub -->
    <test.TestBean version='1.0.0' beanClass='test.TestBean' id="SubSub">
        <name dataType='String'>SubSub</name>
        <kind dataType='String'>Unit</kind>
        <parent beanRef='test.TestBean' domain='_,_,_,parents_domain.groovy'><id>SubB</id></parent>
        <successful dataType='Boolean'>false</successful>
        <cost dataType='Double'>100.0</cost>
        <runs itemClass='test.TestRunComponent' itemName="run">
            <run beanClass='test.TestRunComponent'>
                <date dataType='Date'>Mon Jan 21 12:00:00 EST 2008</date>
                <tester dataType='String'>Bob A. Sess</tester>
            </run>
        </runs>
        <subTests itemClass='BeanReference' itemName="test" itemDomain='_,_,_,subTests_domain.groovy'/>
    </test.TestBean>
</beans>
<?xml version="1.0" encoding="UTF-8"?>

<module>
    <identity>
		<uri>urn:com:mindalliance:data:test</uri>
		<version>0.1.0</version>
    </identity>
	<info>
	    <name>Mind-Alliance Data Library Tests</name>
	    <description>Tests for mod-data</description>
	</info>
    <publisher>
        <name>Mind-Alliance, Inc.</name>
		<uri>http://www.mindalliance.com</uri>
    </publisher>
    <export>
		<uri>
			<match>ffcpl:/test/mod-data-test/.*</match>
			<match>ffcpl:/entrypoints.xml</match>
		</uri>
    </export>
    <rewrite>
		<rule>
			<match>ffcpl:/test/mod-data-test/xunit(.*)</match>
			<to>active:xunit+testlist@ffcpl:/test/xunit/testlist.xml$1</to>
		</rule>
		<rule>
			<match>ffcpl:/test/mod-data-test/(.*)</match>
			<to>ffcpl:/test/$1</to>
		</rule>
		<rule>
			<match>(.*?js.*)</match>
			<to>active:javascript+operator@$1</to>
		</rule>
	</rewrite>
    <mapping>
		<this>
			<match>ffcpl:/(entrypoints.xml|test/.*)</match>
		</this>
		<import>
			<uri>urn:com:mindalliance:data</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:layer1</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:core</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:ura</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:sys</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:dpml</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:script</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:http:client</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xunit</uri>
		</import>
		<super/>
    </mapping>
</module>
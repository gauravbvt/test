<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!-- The acquisition of information from execting a task -->
  <!--
  start =
    element acquirement {
      {isna_element} &
      {identified} &
      {categorized} &
      {documented} &
      {product} &
      {information}
    }
  -->
  <include href="http://localhost:@localport@/channels/schema/def_all.rng"/>
    
  <start>
    <element name="acquirement">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="categorized"/>
        <ref name="documented"/>
        <ref name="product"/>
        <ref name="information"/>
      </interleave>
    </element>
  </start>
</grammar>
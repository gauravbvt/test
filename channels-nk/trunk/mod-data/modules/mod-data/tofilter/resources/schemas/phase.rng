<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!-- A phase in a scenario -->
  <!--
  start =
    element phase {
      {isna_element} &
      {identified} &
      {documented} &
      element eventId {text}
    }
  -->
  <include href="http://localhost:@localport@/channels/schema/def_all.rng"/>
    
  <start>
    <element name="phase">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="documented"/>
        <element name="eventId"><text/></element>
      </interleave>
    </element>
  </start>
</grammar>
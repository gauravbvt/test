<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--
  start =
      element scenario {
  	    {isna_element},
  	    {identified} & 
        {documented} &
        element projectId {text}
      }
  -->
  <include href="http://localhost:@localport@/channels/schema/def_all.rng"/>
  
  <start>
    <element name="scenario">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="documented"/>
        <element name="projectId"><text/></element>
      </interleave>
    </element>
  </start>
</grammar>
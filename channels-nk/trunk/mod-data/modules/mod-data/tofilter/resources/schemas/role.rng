<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--  
  start =
    element role {
      {all} &
      {identified} &
      {categorized} &
      element organizationId {text} &
      element expertise {
        {information}*
      }
    }
      -->
  <include href="http://@host@:@port@/channels/schema/def_all.rng"/>
 
  <start>
    <element name="role">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="categorized"/>
        <element name="organizationId"><text/></element>
        <element name="expertise">
          <zeroOrMore>
            <ref name="information"/>
          </zeroOrMore>
        </element>
      </interleave>
    </element>
  </start>
</grammar>
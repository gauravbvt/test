<grammar xmlns="http://relaxng.org/ns/structure/1.0">
<!--
  # An assertion that someone or something knows about something within the scenario
    element know {
      {isna_element} &
      {knowing}
  }
-->
  <include href="http://localhost:@localport@/channels/schema/def_all.rng"/>
  
  <start>
    <element name="know">
      <interleave>
        <ref name="isna_element"/>
        <ref name="knowing"/>
      </interleave>
    </element>
  </start>
</grammar>
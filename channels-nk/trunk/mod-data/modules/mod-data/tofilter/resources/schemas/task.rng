<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--  
  start =
    element task {
      {all} &
      {identified} &
      {categorized} &
      {occurrence} &
      {duration}
    }
  -->
  <include href="http://@host@:@port@/channels/schema/def_all.rng"/>
  
  <start>
    <element name="task">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="categorized"/>
        <ref name="occurrence"/>
        <ref name="duration"/>
      </interleave>
    </element>
  </start>
</grammar>
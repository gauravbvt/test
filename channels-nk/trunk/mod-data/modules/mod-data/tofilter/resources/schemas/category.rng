<grammar xmlns="http://relaxng.org/ns/structure/1.0">
<!--
  start =
    element category {
      {taxonomy} &
      {isna_element} &
      {identified} &
      {documented} &
      element disciplines {
        element categoryId {text}*
      }
      element implies {
        element categoryId {text}*
      } &
      {information}
-->
  <include href="@protocol@://@host@:@port@/channels/schema/def_all.rng"/>

    <start>
    <element name="category">
      <interleave>
        <ref name="taxonomy"/>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="documented"/>
        <element name="disciplines">
          <zeroOrMore>
            <element name="categoryId"><text/></element>
          </zeroOrMore>
        </element>
        <element name="implies">
          <zeroOrMore>
            <element name="categoryId"><text/></element>
          </zeroOrMore>
        </element>
        <ref name="information"/>
      </interleave>
    </element>
  </start>
</grammar>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--
    element agent {
      {isna_element} &
      {identified} &
      element taskId {text} &
      element roleId {text}
    }
    -->
  <include href="http://@host@:@port@/channels/schema/def_all.rng"/>

    <start>
    <element name="agent">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <element name="taskId"><text/></element>
        <element name="roleId"><text/></element>
      </interleave>
    </element>
  </start>
</grammar>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--
  start =
    element organization {
      {isna_element} &
      {identified} &
      {categorized}
      {documented} &
      element abbreviation {text}? &
      element logo {text}? &
      element parentOrganizationId {text}? &
      {address}?
    }
  -->
  <include href="@protocol@://@host@:@port@/channels/schema/def_all.rng"/>
  
  <start>
    <element name="organization">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="categorized"/>
        <ref name="documented"/>
        <optional>
          <element name="abbreviation"><text/></element>
        </optional>
        <optional>
          <element name="logo"><text/></element>
        </optional>
        <optional>
          <element name="parentOrganizationId"><text/></element>          
        </optional>
        <optional>
          <ref name="address"/>
        </optional>
      </interleave>
    </element>
  </start>
  
</grammar>
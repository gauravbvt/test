<grammar xmlns="http://relaxng.org/ns/structure/1.0">
<!--
  spatialCoordinates =
    element spatialCoordinates {
      {latlong} | {address}
    }
-->
  <define name="spatialCoordinates">
    <element name="spatialCoordinates">
      <choice>
        <ref name="address"/>
        <ref name="latlong"/>
      </choice>
    </element>
  </define>
</grammar>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--
    level = 
      element level {"low" | "medium" | "high"}
  -->
  <define name="level">
    <element name="level">
      <choice>
        <value>low</value>
        <value>medium</value>
        <value>high</value>
      </choice>
    </element>
  </define>
</grammar>
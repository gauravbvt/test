<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <!--
  start =
    element artifact {
      {all} &
      {identified} &
      {categorized} &
      {product}
}
  -->
  <include href="http://@host@:@port@/channels/schema/def_all.rng"/>
  
  <start>
    <element name="artifact">
      <interleave>
        <ref name="isna_element"/>
        <ref name="identified"/>
        <ref name="categorized"/>
        <ref name="product"/>
      </interleave>
    </element>
  </start>
</grammar>
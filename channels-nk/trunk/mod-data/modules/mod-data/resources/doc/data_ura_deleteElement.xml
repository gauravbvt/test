	<section name="title" styleuri="/xusr/share/doc/styles_1060/style_title.xsl">
		<content>
			<name>deleteElement</name>
			<text>Accessor to delete an element in the opened model</text>
		</content>
	</section>
	
	<section name="definition" styleuri="/xusr/share/doc/styles_1060/style_definition.xsl">
		<content>
			<module>urn:com:mindalliance:channels:data</module>
			<format>
				<sub name="format"/>
			</format>
			<returns>
				<b>t</b>
			</returns>
			<syntax>
				<type><b>deleteElement</b></type>
				<id req="true">the element's id</id>
				<target req="false">boolean response</target>
			</syntax>
		</content>
		<includes>
			<include name="format" type="xml">
		    <instr>
		      <type>deleteElement</type>
		      <id>1230980875AD45A6BC</id>
		      <target>this:response</target>
		    </instr>
			</include>
		</includes>
	</section>
	
	<section name="purpose" styleuri="/xusr/share/doc/styles_1060/style_basic.xsl">
		<content>
	    <p>
				The <code>deleteElement</code> accessor removes the element with the given unique id from the currently opened model.
			</p>
			<p>If successful the response is the boolean true</p>
		</content>
		<includes>
			<include/>
		</includes>
	</section>

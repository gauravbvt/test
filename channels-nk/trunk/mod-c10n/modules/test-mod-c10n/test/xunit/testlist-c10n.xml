        </assert>
    </test>
    <test>
        <uri>active:javascript+operator@ffcpl:/test/accessors/continuation_update_source.js</uri>
        <assert>
            <xpath>//b = 't'</xpath>
        </assert>
    </test>
    <test>
        <uri>active:javascript+operator@ffcpl:/test/accessors/continuation_create_from_continuation.js</uri>
        <assert>
            <xpath>//b = 't'</xpath>
        </assert>
    </test>
</testlist>
<?xml version="1.0" encoding="UTF-8"?>

<module>
    <identity>
		<uri>urn:com:mindalliance:channels:c10n:test</uri>
		<version>0.0.1</version>
    </identity>
	<info>
	    <name>Channels C10n Library Tests</name>
	    <description>Tests for mod-c10n</description>
	</info>
    <publisher>
        <name>Mind-Alliance, Inc.</name>
		<uri>http://www.mindalliance.com</uri>
    </publisher>
    <export>
		<uri>
			<match>ffcpl:/test/mod-c10n-test/.*</match>
			<match>ffcpl:/(entrypoints.xml|etc/.*)</match>
		</uri>
    </export>
    <rewrite>
		<rule>
			<match>ffcpl:/test/mod-c10n-test/xunit(.*)</match>
			<to>active:xunit+testlist@ffcpl:/test/xunit/testlist.xml$1</to>
		</rule>
		<rule>
			<match>ffcpl:/test/mod-c10n-test/(.*)</match>
			<to>ffcpl:/test/$1</to>
		</rule>
	</rewrite>
    <mapping>
		<this>
			<match>ffcpl:/(entrypoints.xml|test/.*|etc/.*)</match>
		</this>
		<import>
			<uri>urn:com:mindalliance:channels:c10n</uri>
		</import>
        <import>
            <uri>urn:com:mindalliance:channels:support</uri>
        </import>
        <import>
            <uri>urn:org:ten60:netkernel:ext:session</uri>
        </import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:layer1</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:core</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:ura</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:sys</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:script</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xunit</uri>
		</import>
		<super/>
    </mapping>
</module>
<?xml version="1.0" encoding="UTF-8"?>

<module>
    <identity>
		<uri>urn:com:mindalliance:channels:rest:test</uri>
		<version>0.1.0</version>
    </identity>
	<info>
	    <name>Mind-Alliance Channels REST Module Tests</name>
	    <description>Tests for mod-rest</description>
	</info>
    <publisher>
        <name>Mind-Alliance, Inc.</name>
		<uri>http://www.mindalliance.com</uri>
    </publisher>
    <export>
      <uri>
        <match>ffcpl:/com/mindalliance/channels/library/.*</match>
        <match>ffcpl:/test/mod-rest-test/.*</match>
        <match>ffcpl:/(entrypoints.xml)</match>
      </uri>
    </export>
    <rewrite>
		  <rule>
        <match>ffcpl:/test/mod-rest-test/xunit(.*)</match>
        <to>active:xunit+testlist@ffcpl:/test/xunit/testlist.xml$1</to>
      </rule>
      <rule>
        <match>ffcpl:/test/mod-rest-test/(.*)</match>
        <to>ffcpl:/test/$1</to>
      </rule>
    </rewrite>
    <mapping>
		  <this>
        <match>ffcpl:/(entrypoints.xml|test/.*)</match>
      </this>
      <import>
        <uri>urn:com:mindalliance:channels:rest</uri>
      </import>
      <import>
        <uri>urn:com:mindalliance:channels:support</uri>
      </import>
       <import>
        <uri>urn:com:mindalliance:channels:data</uri>
      </import>
     <import>
        <uri>urn:org:ten60:netkernel:ext:layer1</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:xml:core</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:xml:ura</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:sys</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:dpml</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:script</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:http:client</uri>
      </import>
      <import>
        <uri>urn:org:ten60:netkernel:ext:xunit</uri>
      </import>
      <super/>
    </mapping>
</module>
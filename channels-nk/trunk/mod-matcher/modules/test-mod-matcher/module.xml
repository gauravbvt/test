<?xml version="1.0" encoding="UTF-8"?>

<module>
    <identity>
		<uri>urn:com:mindalliance:channels:matcher:test</uri>
		<version>0.0.1</version>
    </identity>
	<info>
	    <name>Mind-Alliance Matcher Library Tests</name>
	    <description>Tests for mod-matcher</description>
	</info>
    <publisher>
        <name>Mind-Alliance, Inc.</name>
		<uri>http://www.mindalliance.com</uri>
    </publisher>
    <export>
		<uri>
			<match>ffcpl:/test/mod-matcher-test/.*</match>
			<match>ffcpl:/(entrypoints.xml)</match>
		</uri>
    </export>
    <rewrite>
		<rule>
			<match>ffcpl:/test/mod-matcher-test/xunit(.*)</match>
			<to>active:xunit+testlist@ffcpl:/test/xunit/testlist.xml$1</to>
		</rule>
        <rule>
            <match>ffcpl:/test/mod-matcher-test/(.*)</match>
            <to>ffcpl:/test/$1</to>
        </rule>
		<rule>
			<match>(.*?bsh.*)</match>
			<to>active:beanshell+operator@$1</to>
		</rule>
	</rewrite>
    <mapping>
        <this>
            <match>ffcpl:/(entrypoints.xml|test/.*)</match>
		</this>
		<import>
			<uri>urn:com:mindalliance:channels:matcher</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:layer1</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:core</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:ura</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:sys</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:dpml</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:script</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:http:client</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xunit</uri>
		</import>
		<super/>
    </mapping>
</module>
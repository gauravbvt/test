<?xml version="1.0" encoding="UTF-8"?>
<testlist  title="Matcher Accessor Unit Tests">
    <desc>
	<div>
	<b>Matcher</b> accessor unit tests for the <b>Mind-Alliance Matcher</b> lib.
	</div>
    </desc>

    <test>
        <uri>active:javascript+operator@ffcpl:/test/accessor/accuracy-test.js</uri>
		<assert>
			<xpath>number(//:td[@id='score']) >= 0.5</xpath>
		</assert>
    </test>

    <test>
        <uri>active:javascript+operator@ffcpl:/test/accessor/categoryTest.js</uri>
		<assert>
			<xpath>number(/result/@value) >= 0.5</xpath>
		</assert>
    </test>

    <test>
        <uri>active:javascript+operator@ffcpl:/test/accessor/sortTest.js</uri>
		<assert>
			<xpath>count(/result/value) >= 1</xpath>
		</assert>
    </test>

</testlist>
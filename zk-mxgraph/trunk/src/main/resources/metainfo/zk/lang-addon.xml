<language-addon>
	<!-- The name of this addon. It must be unique -->
	<addon-name>mxgraph</addon-name>
	<!-- Specifies what other addon this depends
	<depends></depends>
	-->

	<version>
		<version-class>com.mindalliance.zk.mxgraph.Version</version-class>
		<version-uid>1.0</version-uid>
	</version>

	<!-- Which language this addon will be added to -->
	<language-name>xul/html</language-name>

	<!-- [Optional, Multiple] Define a JavaScript module.
		Change the version if JS files are modified, such that
		browsers will reload them. -->
	<javascript-module name="mxgraphz.mxgraph" version="1.0"/>

	<zscript>
		import com.mindalliance.zk.mxgraph.*;
	</zscript>

	<component>
	    <component-name>mxgraph</component-name>
    	<component-class>com.mindalliance.zk.mxgraph.MxGraph</component-class>
    	<mold>
        	<mold-name>default</mold-name>
        	<mold-uri>~./mxgraph/mxgraph.dsp</mold-uri>
    	</mold>
	</component>

</language-addon>

